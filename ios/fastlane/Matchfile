git_url(ENV['MATCH_REPO_URL'])
storage_mode("git")

type("appstore") # The default type, can be: appstore, adhoc, enterprise or development
verbose(true)
skip_certificate_matching(true)
app_identifier(ENV['FASTLANE_APP_IDENTIFIER'])
