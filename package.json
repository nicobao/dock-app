    "start": "APP_RUNTIME=default && react-native start",
    "start:storybook": "APP_RUNTIME=storybook && react-native start",
    "test": "jest --coverage --collectCoverageFrom=src/**/**/*.{js,jsx,ts,tsx} ./src/** --maxWorkers=2 ",
    "test:e2e": "detox test",
    "build:e2e": "detox build",
